    "react-dom": "19.1.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "cpy-cli": "^5.0.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4"
  }
}
